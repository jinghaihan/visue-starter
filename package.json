{
  "name": "visue-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --noEmit",
    "build:pkg": "turbo build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test:unit": "vitest",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "prepare": "pnpm build:pkg",
    "taze": "npx taze -r --exclude pnpm",
    "taze:up": "taze major -I --exclude pnpm"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@unhead/vue": "^1.11.9",
    "@unocss/reset": "^0.63.4",
    "@visue/preset-antd": "workspace:*",
    "@vueuse/core": "^11.1.0",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.13",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "radix-vue": "^1.9.7",
    "tailwind-merge": "^2.5.4",
    "theme-colors": "^0.1.0",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@iconify-json/lucide": "^1.2.9",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@shikijs/markdown-it": "^1.22.0",
    "@types/dom-view-transitions": "^1.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^0.63.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "markdown-it-link-attributes": "^4.0.1",
    "postcss": "^8.4.47",
    "postcss-nested": "^6.2.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.17.2",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3",
    "unocss": "^0.63.4",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-radix": "^2.5.5",
    "unocss-preset-shadcn": "^0.3.1",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
